{
	"name": "baltimare-leaderboard",
	"license": "AGPL-3.0-only",
	"packageManager": "pnpm@9.7.0",
	"scripts": {
		"start": "concurrently npm:start:*",
		"start:server": "tsx --watch server/main.ts",
		"start:frontend": "parcel watch --watch-dir frontend --dist-dir frontend/dist frontend/index.html",
		"build": "rimraf frontend/dist && parcel build --dist-dir frontend/dist frontend/index.html",
		"serve": "tsx server/main.ts"
	},
	"parcelIgnore": [
		".*\\.png",
		".*\\.jpg"
	],
	"dependencies": {
		"@seald-io/nedb": "^4.0.4",
		"croner": "^8.1.1",
		"express": "^4.19.2",
		"jsdom": "^24.1.1",
		"tsx": "^4.17.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.0",
		"@emotion/babel-plugin": "^11.12.0",
		"@emotion/react": "^11.13.0",
		"@emotion/styled": "^11.13.0",
		"@parcel/config-default": "^2.12.0",
		"@types/express": "^4.17.21",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.2.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"concurrently": "^8.2.2",
		"date-fns": "^3.6.0",
		"parcel": "^2.12.0",
		"parcel-resolver-ignore": "^2.2.0",
		"process": "^0.11.10",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.2.1",
		"rimraf": "^6.0.1"
	}
}
